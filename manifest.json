{
  "manifest_version": 3,
  "name": "Multi Find & Replace",
  "version": "1.0",
  "description": "Find and replace multiple text patterns on any webpage",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
